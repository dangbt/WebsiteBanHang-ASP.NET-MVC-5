<style>
    .list_of_members {
        display: none;
    }
</style>
<br />
<div class="list_of_members1">
    <div class="sales">
       
        <div class="icon-text">
            @{decimal TongDoanhThu = decimal.Parse(ViewBag.TongDoanhThu.ToString());
                TongDoanhThu = TongDoanhThu / 1000;
            }
            <h3>@TongDoanhThu.ToString("#,##")K <i class="fa fa-usd" aria-hidden="true"></i>  </h3>     
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="new-users">
      
        <div class="icon-text">
            <h3>@ViewBag.TongThanhVien <i class="fa fa-user-circle" aria-hidden="true"></i></h3>
          
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="orders">
      
        <div class="icon-text">
            <h3>@ViewBag.TongDDH <i class="fa fa-taxi" aria-hidden="true"></i></h3>
          
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="visitors">
       
        <div class="icon-text">
            <h3>@ViewBag.SoNguoiTruyCap <i class="fa fa-eye" aria-hidden="true"></i></h3>
           
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="clear"></div>
    <div class="visitors">
       
        <div class="icon-text">
            <h3>@ViewBag.SoLuongNguoiOnLine <i class="fa fa-users" aria-hidden="true"></i></h3>
           
        </div>      
    </div>
    <div class="clearfix"></div>
</div>
